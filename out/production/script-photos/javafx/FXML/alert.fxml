<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" id="alert" fx:id="text"
            style="-fx-background-radius: 20px; -fx-border-radius: 20px;" stylesheets="@../../css/main.css"
            xmlns="http://javafx.com/javafx/11.0.1" fx:controller="javafx.AlertDialogController">
    <children>
        <VBox alignment="CENTER" layoutX="2.0" layoutY="2.0" spacing="10.0" AnchorPane.bottomAnchor="0.0"
              AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <Label fx:id="textAlert" alignment="CENTER" text="2 empty folders were deleted" textAlignment="CENTER">
                    <font>
                        <Font name="Arial" size="18.0"/>
                    </font>
                </Label>
                <Button id="dialogButton" fx:id="okButton" mnemonicParsing="false" onAction="#okAction"
                        stylesheets="@../../css/main.css" text="OK"/>
            </children>
            <padding>
                <Insets bottom="10.0" left="15.0" right="15.0" top="15.0"/>
            </padding>
        </VBox>
    </children>
</AnchorPane>
